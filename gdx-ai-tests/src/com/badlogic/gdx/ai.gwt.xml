<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit trunk//EN" "http://www.gwtproject.org/doctype/2.8.2/gwt-module.dtd">
<module>
	<!-- This whole file won't be used by the tests here, but it can be useful
	     to know what to put in your own .gwt.xml files. Behavior tree tasks are
	     loaded via reflection, so they need gdx.reflect.include set for them.
	     The GUI here also needs reflection, which isn't related to gdx-ai. -->
	<source path="ai"/>
	<extend-configuration-property name="gdx.reflect.include" value="com.badlogic.gdx.ai.tests.utils.scene2d" />
	<extend-configuration-property name="gdx.reflect.include" value="com.badlogic.gdx.ai.tests.btree.dog" />
</module>
